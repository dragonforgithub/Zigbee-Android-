        android:layout_height="250dp"
        android:layout_gravity="right|bottom"
        android:layout_marginBottom="80dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/main_every_bg"
        android:orientation="vertical" >
    </LinearLayout>

